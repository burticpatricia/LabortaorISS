<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="mo.model">

    <class name="Produs" table="Produs">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="denumire" column="denumire"/>
        <property name="producator" column="producator"/>
        <property name="pret" column="pret"/>
        <property name="stoc" column="stoc"/>
    </class>

</hibernate-mapping>


